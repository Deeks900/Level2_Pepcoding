#include<bits/stdc++.h>
using namespace std;

bool isPowerOfFour(int n){
   if(n == 0)
   return false;

   while(n != 1){
       if(n%4 != 0){
           return false;
       }
       n = n/4;
   }
   return true;
}

int main(){
    int n;
    cin>>n;
    if(isPowerOfFour(n))
    cout<<"true";
    else cout<<"false";
    
}
