#include<bits/stdc++.h>
using namespace std;

void exceptOne(int* arr, int n){
    int res = 0;
    for(int i = 0; i < n; i++){
        res = res^arr[i];
    }
    
    cout << res;
}

int main(){
    int n;
    cin >> n;
    
    int* arr = new int[n];
    for(int i = 0; i < n; i++){
        cin >> arr[i];
    }
    
    exceptOne(arr, n);
    return 0;
}
